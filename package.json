{
    "version": "10.0.0",
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "migrate": "node ./dist/migrate.js --up",
        "rollback": "node ./dist/migrate.js --down",
        "watch": "tsc --watch --noEmit",
        "build": "tsc --build",
        "lint": "eslint . --fix --max-warnings=0 --cache",
        "format": "dprint fmt",
        "type-check": "tsc --noEmit",
        "quality": "concurrently -n ESLint,dprint,TypeScript -c #3178C6,#4D5564,#4B32C3 \"npm run lint\" \"npm run format\" \"npm run type-check\" "
    },
    "dependencies": {
        "axios": "^1.13.2",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@stylistic/eslint-plugin": "^5.6.1",
        "@types/lodash": "^4.17.21",
        "@types/node": "^25.0.0",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "concurrently": "^9.2.1",
        "dprint": "^0.50.2",
        "eslint": "^9.39.1",
        "eslint-plugin-check-file": "^3.3.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.47.0"
    }
}
